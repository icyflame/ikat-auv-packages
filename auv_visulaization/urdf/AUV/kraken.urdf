  <link name="auv_body">
    <inertial>
      <origin xyz="0 0 0" /> 
      <mass value="35" />
      <inertia  ixx="1" ixy="0.0"  ixz="0.0"  iyy="1"  iyz="0.0"  izz="1" />
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://auv_visulaization/urdf/AUV/mesh_files/new_model_kraken.stl"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://auv_visulaization/urdf/AUV/mesh_files/new_model_kraken.stl"/>
      </geometry>
    </collision>
