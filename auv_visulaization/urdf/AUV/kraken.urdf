  <gazebo reference="auv_body">
    <material>Gazebo/Green</material>
    <turnGravityOff>true</turnGravityOff>
  </gazebo>
  <link name="right_heave_thruster">
    <inertial>
      <origin xyz="0 0 0" /> 
      <mass value="0.01" />
      <inertia  ixx="0.01" ixy="0.0"  ixz="0.0"  iyy="0.01"  iyz="0.0"  izz="0.01" />
    </inertial>
    <visual>
